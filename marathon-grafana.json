{
    "acceptedResourceRoles": [
        "slave_public"
    ],
    "constraints": [
        [
            "hostname",
            "UNIQUE"
        ]
    ],
    "container": {
        "docker": {
            "image": "grafana/grafana:2.6.0",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 0
                }
            ]
        },
        "type": "DOCKER"
    },
    "cpus": 0.2,
    "id": "grafana",
    "mem": 300
}
